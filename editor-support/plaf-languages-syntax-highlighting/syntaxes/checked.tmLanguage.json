{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Checked",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#constants"
		},
		{
			"include": "#functions"
		},
		{
			"include": "#types"
		},
		{
			"include": "#strings"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [
				{
					"name": "keyword.control.checked",
					"match": "\\b(if|then|else|caseT)\\b"
				},
				{
					"name": "keyword.storage.checked",
					"match": "\\b(let|letrec|pair|unpair|untuple)\\b"
				},
				{
					"name": "keyword.other.checked",
					"match": "\\b(begin|end|in|of|proc)\\b"
				}
			]
		},
		"constants": {
			"patterns":[
				{
					"name": "constant.numeric.checked",
					"match": "-?[0-9]+"
				}
			]
		},
		"functions": {
			"patterns": [
				{
					"name": "support.function.checked",
					"match": "\\b(((zero\\?|empty\\?)\\W)|((debug|fst|snd|cons|hd|tl|emptylist|node|emptytree)\\b))"
				}
			]
		},
		"types": {
			"patterns": [
				{
					"name": "support.type.checked",
					"match": "\\b(unit|bool|int|list|tree|ref)\\b"
				}
			]
		},
		"strings": {
			"name": "string.quoted.double.checked",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.checked",
					"match": "\\\\."
				}
			]
		}
	},
	"scopeName": "source.chk"
}